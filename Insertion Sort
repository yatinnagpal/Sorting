#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
      cin>>a[i];
    }
    for(int i=0;i<n-1;i++)
    {
        for(int j=i+1;j>0;j--)
        {
            if(a[j-1]>a[j])
            {
                int temp=a[j];
                a[j]=a[j-1];
                a[j-1]=temp;
            }
        }
    }
    cout<<"\nAfter sorting :\n";
    for(int i=0;i<n;i++)
    cout<<a[i]<<" ";
    
    cout<<"After sorting elements :\n";
    for(int i=0;i<n;i++)
    {
        cout<<a[i]<<" ";
    }
}
    
