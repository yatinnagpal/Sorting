#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a[]={3,5,8,7,11,2,9,13};
    int n=8;
    for(int i=1;i<n;i++)
    {
        for(int j=i-1;j>=0;j--)
        {
            if(a[j]>a[j+1])
            swap(a[j],a[j+1]);
        }
    }
    for(int i=0;i<n;i++)
    cout<<a[i]<<" ";
}
